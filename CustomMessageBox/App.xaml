<Application x:Class="CustomMessageBox.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary
                    Source="pack://application:,,,/RQ.Windows;Component/Styles/StylesMergedDictionary.xaml" />
                <ResourceDictionary
                    Source="pack://application:,,,/RQ.Windows;Component/Styles/Controls/TextBlock.xaml" />
                <ResourceDictionary
                    Source="General.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
